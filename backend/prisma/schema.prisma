

generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Food {
  id          Int      @id    @default    (autoincrement())
  name        String
  calories    Int
  protein     Int
  carbs       Int
  fats        Int
  unit        String
  category    String?
  createdAt   DateTime         @default(now())
  updatedAt   DateTime         @updatedAt
}


model User {  
  id               Int     @id @default(autoincrement())
  email           String   @unique
  name            String
  password        String
  createdAt       DateTime @default(now())  
  updatedAt       DateTime @updatedAt
}

